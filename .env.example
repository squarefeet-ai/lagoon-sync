#------------------------------------------------------------------------------
# Lagoon Sync – Environment Configuration Example
# Copy this file to `.env` and replace the placeholder values.
#------------------------------------------------------------------------------

# ---------- Source (READ) ----------------------------------------------------
LAGOON_SOURCE_ENDPOINT_URL="https://s3.us-east-1.wasabisys.com"
LAGOON_SOURCE_ACCESS_KEY_ID="YOUR_SOURCE_ACCESS_KEY"
LAGOON_SOURCE_SECRET_ACCESS_KEY="YOUR_SOURCE_SECRET_KEY"
LAGOON_SOURCE_BUCKET="your-source-bucket"
# Optional (default: us-east-1)
LAGOON_SOURCE_REGION="us-east-1"

# ---------- Destination (WRITE) ---------------------------------------------
LAGOON_DESTINATION_ENDPOINT_URL="http://localhost:9000"
LAGOON_DESTINATION_ACCESS_KEY_ID="YOUR_DEST_ACCESS_KEY"
LAGOON_DESTINATION_SECRET_ACCESS_KEY="YOUR_DEST_SECRET_KEY"
LAGOON_DESTINATION_BUCKET="your-destination-bucket"
# Optional (default: us-east-1)
LAGOON_DESTINATION_REGION="us-east-1"